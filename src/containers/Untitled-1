                                moves:
                                <MoveTab
                                    listView={listView}
                                    selectedCharacterMoves={selectedCharacterMoves}
                                    navigation={navigation}
                                    theme={theme}
                                    label={label}
                                    updateMoveData={updateMoveData}
                                />,
                                combos: <Text>Combos</Text>,
